<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quanly.controller.covid.GhiNhanThongTinCachLyController">
   <content>
      <GridPane prefHeight="257.0" prefWidth="306.0">
          <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES" maxWidth="193.0" minWidth="10.0" prefWidth="146.0" />
              <ColumnConstraints hgrow="SOMETIMES" maxWidth="260.0" minWidth="10.0" prefWidth="250.0" />
          </columnConstraints>
          <rowConstraints>
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
              <Label text="Mã cách ly" />
              <Label text="Nhân khẩu" GridPane.rowIndex="1" />
              <Label prefHeight="18.0" prefWidth="99.0" text="Địa điểm cách ly" GridPane.rowIndex="3" />
              <Label text="Từ ngày" GridPane.rowIndex="4" />
              <Label text="Đến ngày" GridPane.rowIndex="5" />
              <Label text="Mức độ cách ly" GridPane.rowIndex="6" />
              <TextField fx:id="maCachLyTF" GridPane.columnIndex="1" />
              <Button fx:id="chonNhanKhauBtn" mnemonicParsing="false" text="Chọn..." GridPane.columnIndex="1" GridPane.rowIndex="1" />
              <TextField fx:id="diaDiemCachLyTF" GridPane.columnIndex="1" GridPane.rowIndex="3" />
              <ChoiceBox fx:id="mucDoCachLyChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
              <DatePicker fx:id="tuNgayDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="4" />
              <DatePicker fx:id="denNgayDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label text="Họ và tên" GridPane.rowIndex="2" />
            <Label fx:id="hoTenLB" GridPane.columnIndex="1" GridPane.rowIndex="2" />
          </children>
      </GridPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="OK" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
</DialogPane>
