<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="219.0" prefWidth="397.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quanly.controller.LoginController">
   <children>
      <Label layoutX="16.0" layoutY="4.0" text="Username">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="67.0" text="Password">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTF" layoutX="14.0" layoutY="42.0" prefHeight="25.0" prefWidth="225.0" />
      <PasswordField fx:id="passwordTF" layoutX="14.0" layoutY="105.0" prefHeight="25.0" prefWidth="225.0" />
      <Button fx:id="loginBtn" layoutX="16.0" layoutY="144.0" mnemonicParsing="false" onAction="#onLoginBtnClick" prefHeight="32.0" prefWidth="86.0" text="Login">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="emptyUsrLabel" layoutX="249.0" layoutY="46.0" text="Chưa nhập username" textFill="RED" />
      <Label fx:id="emptyPwdLabel" layoutX="249.0" layoutY="109.0" text="Chưa nhập password" textFill="RED" />
      <Label fx:id="accessRefusedLabel" layoutX="16.0" layoutY="188.0" text="Sai username hoặc password" textFill="RED" />
   </children>
</AnchorPane>
