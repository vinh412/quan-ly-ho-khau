<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="chon_thanh_vien_layout" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="60.0" prefWidth="880.0">
        <TextField fx:id="searchTF" layoutX="14.0" layoutY="8.0" prefHeight="46.0" prefWidth="282.0" />
        <ComboBox fx:id="comboBox" layoutX="309.0" layoutY="7.0" prefHeight="46.0" prefWidth="150.0" />
        <Button fx:id="timKiemBtn" layoutX="467.0" layoutY="7.0" mnemonicParsing="false" onAction="#onTimKiemBtnClick" prefHeight="46.0" prefWidth="80.0" text="Tìm kiếm" />
      <TextField fx:id="quanHeTF" layoutX="710.0" layoutY="8.0" prefHeight="46.0" prefWidth="156.0" />
      <Label layoutX="593.0" layoutY="22.0" text="Quan hệ với chủ hộ" />
    </AnchorPane>
    <TableView fx:id="tableView" prefHeight="464.0" prefWidth="880.0">
        <columns>
            <TableColumn fx:id="maNhanKhau" text="Mã nhân khẩu" />
            <TableColumn fx:id="hoTen" text="Họ tên" />
            <TableColumn fx:id="gioiTinh" text="Giới tính" />
            <TableColumn fx:id="namSinh" text="Năm sinh" />
            <TableColumn fx:id="diaChiHienNay" text="Địa chỉ hiện nay" />
        </columns>
    </TableView>
    <Button fx:id="chonBtn" layoutX="657.0" layoutY="8.0" mnemonicParsing="false" onAction="#onChonBtnClick" prefHeight="46.0" prefWidth="113.0" text="Chọn" />
</VBox>
